<?php

namespace Amd\PiperBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Tclave
 *
 * @ORM\Table(name="tclave")
 * @ORM\Entity(repositoryClass="Amd\PiperBundle\Repository\TclaveRepository")
 */
class Tclave
{
    /**
     * @var int
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

   
    /**
     * @var string
     *
     * @ORM\Column(name="descriptc", type="string", length=255)
     */
    private $descriptc;

    /**
     * @var string
     *
     * @ORM\Column(name="cdebe", type="string", length=255)
     */
    private $cdebe;

    /**
     * @var string
     *
     * @ORM\Column(name="chaber", type="string", length=255)
     */
    private $chaber;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set cleve
     *
     * @param string $cleve
     * @return Tclave
     */
    public function setCleve($cleve)
    {
        $this->cleve = $cleve;

        return $this;
    }

    /**
     * Get cleve
     *
     * @return string 
     */
    public function getCleve()
    {
        return $this->cleve;
    }

    /**
     * Set descriptc
     *
     * @param string $descriptc
     * @return Tclave
     */
    public function setDescriptc($descriptc)
    {
        $this->descriptc = $descriptc;

        return $this;
    }

    /**
     * Get descriptc
     *
     * @return string 
     */
    public function getDescriptc()
    {
        return $this->descriptc;
    }

    /**
     * Set cdebe
     *
     * @param string $cdebe
     * @return Tclave
     */
    public function setCdebe($cdebe)
    {
        $this->cdebe = $cdebe;

        return $this;
    }

    /**
     * Get cdebe
     *
     * @return string 
     */
    public function getCdebe()
    {
        return $this->cdebe;
    }

    /**
     * Set chaber
     *
     * @param string $chaber
     * @return Tclave
     */
    public function setChaber($chaber)
    {
        $this->chaber = $chaber;

        return $this;
    }

    /**
     * Get chaber
     *
     * @return string 
     */
    public function getChaber()
    {
        return $this->chaber;
    }
}
