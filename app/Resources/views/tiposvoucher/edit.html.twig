{% extends "layout.html.twig" %}
{% block navegacion %}
        <ul class="breadcrumb">
            <li><i class="icon-home"></i><a href=""> Inicio</a> <span class="divider"><i class="icon-angle-right"></i></span></li>
                <li class="active">CONTABLE</li> 
            <li class="moveDown pull-right">
                
                <span class="time"></span>
                <span class="date"></span>
            </li>    
        </ul>
{% endblock navegacion %}



{% block contenido %}
    
    <div class="container-fluid"> 
    <div class="row-fluid">
      <div class="span6">
        <div class="widget-box">
          <div class="widget-title"> 
            <h5>Ingreso Tabla Tipos de Voucher</h5>
          </div>
          <div class="widget-content nopadding">
           <form action="{{ path('tiposvoucher_edit',{'id':tiposvoucher.id}) }}" method="post" {{ form_enctype(edit_form) }} class="form-horizontal">
              <div class="control-group">
                <label class="control-label">{{form_label(edit_form.descripcion)}}</label>
                <div class="controls">
                  {{form_widget(edit_form.descripcion)}}
                </div>
              </div>

              <div class="form-actions">
                <a  href="{{path('tiposvoucher_index') }}"  <button class="btn btn-warning" type="button"> Regresar al Listado</button></a>  
                <button type="submit" class="btn btn-success">Insertar</button>
              </div>
            {{ form_widget(edit_form) }}
            </form>
          </div>
        </div>
         
{% endblock contenido %}
